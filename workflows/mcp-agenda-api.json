{
  "name": "MCP para API Agenda",
  "nodes": [
    {
      "parameters": {},
      "name": "Start",
      "type": "n8n-nodes-base.start",
      "typeVersion": 1,
      "position": [
        250,
        300
      ]
    },
    {
      "parameters": {
        "path": "agenda",
        "options": {}
      },
      "name": "MCP Server Trigger",
      "type": "n8n-nodes-base.mcpServerTrigger",
      "typeVersion": 1,
      "position": [
        450,
        300
      ],
      "webhookId": "webhook-mcp-agenda"
    },
    {
      "parameters": {
        "url": "={{$json.body.url || 'https://agenda.anthonymax.com/api/'}}",
        "authentication": "predefinedCredential",
        "credentialType": "httpHeaderAuth",
        "headerAuth": {
          "name": "Authorization",
          "value": "={{$json.body.token || ''}}"
        },
        "method": "={{$json.body.method || 'GET'}}",
        "sendBody": true,
        "body": "={{$json.body.data}}",
        "options": {
          "fullResponse": true
        }
      },
      "name": "API Request",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 2,
      "position": [
        700,
        300
      ]
    }
  ],
  "connections": {
    "MCP Server Trigger": {
      "main": [
        [
          {
            "node": "API Request",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {},
  "id": "1"
} 